[{"/Users/lavrton/Projects/polotno-studio/src/index.js":"1","/Users/lavrton/Projects/polotno-studio/src/App.js":"2","/Users/lavrton/Projects/polotno-studio/src/serviceWorker.js":"3","/Users/lavrton/Projects/polotno-studio/src/topbar.js":"4"},{"size":1384,"mtime":1610506003625,"results":"5","hashOfConfig":"6"},{"size":1088,"mtime":1608322855134,"results":"7","hashOfConfig":"6"},{"size":5086,"mtime":1602547816843,"results":"8","hashOfConfig":"6"},{"size":7479,"mtime":1610985484666,"results":"9","hashOfConfig":"6"},{"filePath":"10","messages":"11","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"lmm6xv",{"filePath":"12","messages":"13","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"14","messages":"15","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"16","messages":"17","errorCount":0,"warningCount":8,"fixableErrorCount":0,"fixableWarningCount":0,"source":"18"},"/Users/lavrton/Projects/polotno-studio/src/index.js",[],"/Users/lavrton/Projects/polotno-studio/src/App.js",[],"/Users/lavrton/Projects/polotno-studio/src/serviceWorker.js",[],"/Users/lavrton/Projects/polotno-studio/src/topbar.js",["19","20","21","22","23","24","25","26"],"import React from 'react';\nimport { observer } from 'mobx-react-lite';\nimport {\n  Button,\n  Navbar,\n  Alignment,\n  AnchorButton,\n  Divider,\n  Dialog,\n  Classes,\n} from '@blueprintjs/core';\nimport FaGithub from '@meronex/icons/fa/FaGithub';\nimport FaDiscord from '@meronex/icons/fa/FaDiscord';\nimport DownloadButton from 'polotno/toolbar/download-button';\n\nimport { downloadFile } from 'polotno/utils/download';\n\nexport default observer(({ store }) => {\n  // const oneSelected = store.selectedElements.length === 1;\n  // const firstSelected = store.selectedElements[0];\n  // const isTextSelected = oneSelected && firstSelected.type === 'text';\n  // const isImageSelected = oneSelected && firstSelected.type === 'image';\n  // const isSvgSelected = oneSelected && firstSelected.type === 'svg';\n  // const noSelection = store.selectedElements.length === 0;\n\n  // const index = store.activePage?.children.indexOf(firstSelected) || 0;\n  // const canMoveUp =\n  //   store.activePage && index < store.activePage.children.length - 1;\n  // const canMoveDown = index > 0;\n\n  const inputRef = React.useRef();\n\n  const [faqOpened, toggleFaq] = React.useState(false);\n\n  return (\n    <Navbar>\n      <Navbar.Group align={Alignment.LEFT}>\n        <Button\n          icon=\"new-object\"\n          minimal\n          onClick={() => {\n            const ids = store.pages\n              .map((page) => page.children.map((child) => child.id))\n              .flat();\n            const hasObjects = ids?.length;\n            if (hasObjects) {\n              if (!window.confirm('Remove all content for a new design?')) {\n                return;\n              }\n            }\n            const pagesIds = store.pages.map((p) => p.id);\n            store.deletePages(pagesIds);\n            store.addPage();\n          }}\n        >\n          New\n        </Button>\n        <label htmlFor=\"load-project\">\n          <Button\n            icon=\"folder-open\"\n            minimal\n            onClick={() => {\n              document.querySelector('#load-project').click();\n            }}\n          >\n            Open\n          </Button>\n          <input\n            type=\"file\"\n            id=\"load-project\"\n            ref={inputRef}\n            style={{ width: '180px', display: 'none' }}\n            onChange={(e) => {\n              var input = e.target;\n\n              var reader = new FileReader();\n              reader.onload = function () {\n                var text = reader.result;\n                const json = JSON.parse(text);\n                store.loadJSON(json);\n              };\n              reader.readAsText(input.files[0]);\n            }}\n          />\n        </label>\n        <Button\n          icon=\"floppy-disk\"\n          minimal\n          onClick={() => {\n            const json = store.toJSON();\n            const url = 'data:text/json,' + JSON.stringify(json);\n            downloadFile(url, 'polotno.json');\n          }}\n        >\n          Save\n        </Button>\n      </Navbar.Group>\n      <Navbar.Group align={Alignment.RIGHT}>\n        <a\n          href=\"https://www.producthunt.com/posts/polotno-studio?utm_source=badge-featured&utm_medium=badge&utm_souce=badge-polotno-studio\"\n          target=\"_blank\"\n        >\n          <img\n            src=\"https://api.producthunt.com/widgets/embed-image/v1/featured.svg?post_id=281373&theme=dark\"\n            alt=\"Polotno Studio - Canva-like design editor, without signups or ads. | Product Hunt\"\n            style={{ height: '30px', marginBottom: '-4px' }}\n          />\n        </a>\n        <AnchorButton\n          minimal\n          href=\"https://github.com/lavrton/polotno-studio\"\n          target=\"_blank\"\n          icon={<FaGithub />}\n        >\n          Github\n        </AnchorButton>\n        <AnchorButton\n          minimal\n          href=\"https://discord.gg/W2VeKgsr9J\"\n          target=\"_blank\"\n          icon={<FaDiscord />}\n        >\n          Join Chat\n        </AnchorButton>\n        <Button icon=\"info-sign\" minimal onClick={() => toggleFaq(true)}>\n          About\n        </Button>\n\n        <Divider />\n        <DownloadButton store={store} />\n        {/* <NavbarHeading>Polotno Studio</NavbarHeading> */}\n      </Navbar.Group>\n      <Dialog\n        icon=\"info-sign\"\n        onClose={() => toggleFaq(false)}\n        title=\"About Polotno Studio\"\n        isOpen={faqOpened}\n        style={{\n          width: '80%',\n          maxWidth: '700px',\n        }}\n      >\n        <div className={Classes.DIALOG_BODY}>\n          <h2>What is Polotno Studio?</h2>\n          <p>\n            <strong>Polotno Studio</strong> - is a web application to create\n            graphical designs. You can mix image, text and illustrations to make\n            social media posts, youtube previews, podcast covers, business cards\n            and presentations.\n          </p>\n          <h2>Is it Open Source?</h2>\n          <p>\n            Partially. The source code is available in{' '}\n            <a href=\"https://github.com/lavrton/polotno-studio\" target=\"_blank\">\n              GitHub repository\n            </a>\n            . The repository doesn't have full source.{' '}\n            <strong>Polotno Studio</strong> is powered by{' '}\n            <a href=\"https://polotno.dev/\" target=\"_blank\">\n              Polonto SDK project\n            </a>\n            . All core \"canvas editor\" functionality are implemented by{' '}\n            <strong>polotno</strong> npm package (which is not open source at\n            the time of writing this text).\n          </p>\n          <p>\n            Polotno Studio is build on top of Polotno SDK to provide a\n            desktop-app-like experience.\n          </p>\n          <h2>Who is making Polotno Studio?</h2>\n          <p>\n            My name is Anton Lavrenov{' '}\n            <a href=\"https://twitter.com/lavrton\" target=\"_blank\">\n              @lavrton\n            </a>\n            . I am founder of Polotno project. As the maintainer of{' '}\n            <a href=\"https://konvajs.org/\" target=\"_blank\">\n              Konva 2d canvas framework\n            </a>\n            , I created several similar apps for different companies around the\n            world. So I decided to compile all my knowledge and experience into\n            reusable Polotno project.\n          </p>\n          <h2>\n            Why Polotno Studio has no signups and no ads? How are you going to\n            support the project financially?\n          </h2>\n          <p>\n            Instead of monetizing the end-user application{' '}\n            <strong>Polotno Studio</strong> I decided to make money around\n            developers tools with{' '}\n            <a href=\"https://polotno.dev/\" target=\"_blank\">\n              Polonto SDK\n            </a>\n            .\n          </p>\n          <p>\n            <strong>Polotno Studio</strong> is a sandbox application and\n            polished demonstration of{' '}\n            <a href=\"https://polotno.dev/\" target=\"_blank\">\n              Polonto SDK\n            </a>{' '}\n            usage.\n          </p>\n          <p>\n            With{' '}\n            <a href=\"https://polotno.dev/\" target=\"_blank\">\n              Polonto SDK\n            </a>{' '}\n            you can build very different application with very different UI.\n          </p>\n        </div>\n        <div className={Classes.DIALOG_FOOTER}>\n          <div className={Classes.DIALOG_FOOTER_ACTIONS}>\n            <Button onClick={() => toggleFaq(false)}>Close</Button>\n          </div>\n        </div>\n      </Dialog>\n    </Navbar>\n  );\n});\n",{"ruleId":"27","severity":1,"message":"28","line":99,"column":9,"nodeType":"29","endLine":102,"endColumn":10},{"ruleId":"27","severity":1,"message":"28","line":154,"column":13,"nodeType":"29","endLine":154,"endColumn":81},{"ruleId":"27","severity":1,"message":"28","line":159,"column":13,"nodeType":"29","endLine":159,"endColumn":60},{"ruleId":"27","severity":1,"message":"28","line":173,"column":13,"nodeType":"29","endLine":173,"endColumn":67},{"ruleId":"27","severity":1,"message":"28","line":177,"column":13,"nodeType":"29","endLine":177,"endColumn":60},{"ruleId":"27","severity":1,"message":"28","line":192,"column":13,"nodeType":"29","endLine":192,"endColumn":60},{"ruleId":"27","severity":1,"message":"28","line":200,"column":13,"nodeType":"29","endLine":200,"endColumn":60},{"ruleId":"27","severity":1,"message":"28","line":207,"column":13,"nodeType":"29","endLine":207,"endColumn":60},"react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" is a security risk: see https://html.spec.whatwg.org/multipage/links.html#link-type-noopener","JSXOpeningElement"]